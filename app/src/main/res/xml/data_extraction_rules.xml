<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="root" path="."/>
        <exclude domain="file" path="my_cache_file"/>
    </cloud-backup>
    <device-transfer>
        <include domain="root" path="."/>
        <exclude domain="file" path="my_device_specific_file"/>
    </device-transfer>
</data-extraction-rules>
